{
    "name": "clipboarddashboard",
    "version": "1.0.0",
    "description": "Saves copied text to buttons.",
    "main": "public/index.html",
    "scripts": {
        "start": "parcel public/index.html --no-hmr --port $PORT",
        "test": "jest",
