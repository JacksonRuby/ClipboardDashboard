        "firebase",
        "oauth",
        "email",
        "password"
    ],
    "author": "Jackson Ruby",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/JacksonRuby/ClipboardDashboard/issues"
    },
    "homepage": "https://github.com/JacksonRuby/ClipboardDashboard#readme",
    "dependencies": {
        "dom": "0.0.3",
        "firebase": "^7.14.2",
        "firebase-admin": "^8.10.0",
        "framework7": "^4.4.6",
        "framework7-icons": "^2.3.1",
        "leaflet": "^1.5.1",
        "material-design-icons": "^3.0.1",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "firebase-tools": "^7.0.2",
        "parcel-bundler": "^1.12.3",
